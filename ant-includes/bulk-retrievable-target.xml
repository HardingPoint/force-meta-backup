<?xml version="1.0" encoding="UTF-8"?>
<project xmlns:sf="antlib:com.salesforce">

  <target name="bulkRetrievable" depends="-setUpMetadataDir">
    <antcall target="-sfBulkRetrieve"><param name="metadataType" value="AccountSharingRules" /></antcall>
    <antcall target="-sfBulkRetrieve"><param name="metadataType" value="AnalyticSnapshot" /></antcall>
    <antcall target="-sfBulkRetrieve"><param name="metadataType" value="ApexComponent" /></antcall>
    <antcall target="-sfBulkRetrieve"><param name="metadataType" value="ApexTrigger" /></antcall>
    <antcall target="-sfBulkRetrieve"><param name="metadataType" value="ApprovalProcess" /></antcall>
    <antcall target="-sfBulkRetrieve"><param name="metadataType" value="AssignmentRules" /></antcall>
    <antcall target="-sfBulkRetrieve"><param name="metadataType" value="AuthProvider" /></antcall>
    <antcall target="-sfBulkRetrieve"><param name="metadataType" value="AutoResponseRules" /></antcall>
    <antcall target="-sfBulkRetrieve"><param name="metadataType" value="CallCenter" /></antcall>
    <antcall target="-sfBulkRetrieve"><param name="metadataType" value="CampaignSharingRules" /></antcall>
    <antcall target="-sfBulkRetrieve"><param name="metadataType" value="CaseSharingRules" /></antcall>
    <antcall target="-sfBulkRetrieve"><param name="metadataType" value="Community" /></antcall>
    <antcall target="-sfBulkRetrieve"><param name="metadataType" value="ContactSharingRules" /></antcall>
    <antcall target="-sfBulkRetrieve"><param name="metadataType" value="CustomApplicationComponent" /></antcall>
    <antcall target="-sfBulkRetrieve"><param name="metadataType" value="CustomLabels" /></antcall>
    <antcall target="-sfBulkRetrieve"><param name="metadataType" value="CustomPageWebLink" /></antcall>
    <antcall target="-sfBulkRetrieve"><param name="metadataType" value="CustomSite" /></antcall>
    <antcall target="-sfBulkRetrieve"><param name="metadataType" value="DataCategoryGroup" /></antcall>
    <antcall target="-sfBulkRetrieve"><param name="metadataType" value="EscalationRules" /></antcall>
    <antcall target="-sfBulkRetrieve"><param name="metadataType" value="FlexiPage" /></antcall>
    <antcall target="-sfBulkRetrieve"><param name="metadataType" value="Flow" /></antcall>
    <antcall target="-sfBulkRetrieve"><param name="metadataType" value="Group" /></antcall>
    <antcall target="-sfBulkRetrieve"><param name="metadataType" value="HomePageComponent" /></antcall>
    <antcall target="-sfBulkRetrieve"><param name="metadataType" value="HomePageLayout" /></antcall>
    <antcall target="-sfBulkRetrieve"><param name="metadataType" value="InstalledPackage" /></antcall>
    <antcall target="-sfBulkRetrieve"><param name="metadataType" value="OpportunitySharingRules" /></antcall>
    <antcall target="-sfBulkRetrieve"><param name="metadataType" value="PostTemplate" /></antcall>
    <antcall target="-sfBulkRetrieve"><param name="metadataType" value="Queue" /></antcall>
    <antcall target="-sfBulkRetrieve"><param name="metadataType" value="QuickAction" /></antcall>
    <antcall target="-sfBulkRetrieve"><param name="metadataType" value="RemoteSiteSetting" /></antcall>
    <antcall target="-sfBulkRetrieve"><param name="metadataType" value="ReportType" /></antcall>
    <antcall target="-sfBulkRetrieve"><param name="metadataType" value="Role" /></antcall>
    <antcall target="-sfBulkRetrieve"><param name="metadataType" value="SamlSsoConfig" /></antcall>
    <antcall target="-sfBulkRetrieve"><param name="metadataType" value="Scontrol" /></antcall>
    <antcall target="-sfBulkRetrieve"><param name="metadataType" value="Settings" /></antcall>
    <antcall target="-sfBulkRetrieve"><param name="metadataType" value="StaticResource" /></antcall>
    <antcall target="-sfBulkRetrieve"><param name="metadataType" value="Workflow" /></antcall>
    
    <!--
         The following types are invalid for a Developer Edition Salesforce Org.
         and will result in a SoapException.

         Solution for now is simply comment them out when running against a 
         Developer Edition.
    -->
    <antcall target="-sfBulkRetrieve"><param name="metadataType" value="AccountTerritorySharingRules" /></antcall>
    <antcall target="-sfBulkRetrieve"><param name="metadataType" value="EntitlementProcess" /></antcall>
    <antcall target="-sfBulkRetrieve"><param name="metadataType" value="EntitlementTemplate" /></antcall>
    <antcall target="-sfBulkRetrieve"><param name="metadataType" value="MilestoneType" /></antcall>
    <antcall target="-sfBulkRetrieve"><param name="metadataType" value="Portal" /></antcall>
    <antcall target="-sfBulkRetrieve"><param name="metadataType" value="Territory" /></antcall>

    <!--
         The following types are invalid for a Unlimited Edition Salesforce Org.
         and will result in a SoapException.
    -->
    <!--
    <antcall target="-sfBulkRetrieve"><param name="metadataType" value="ArticleType" /></antcall>
    <antcall target="-sfBulkRetrieve"><param name="metadataType" value="LiveChatAgentConfig" /></antcall>
    <antcall target="-sfBulkRetrieve"><param name="metadataType" value="LiveChatButton" /></antcall>
    <antcall target="-sfBulkRetrieve"><param name="metadataType" value="LiveChatDeployment" /></antcall>
    <antcall target="-sfBulkRetrieve"><param name="metadataType" value="Network" /></antcall>
    <antcall target="-sfBulkRetrieve"><param name="metadataType" value="Skill" /></antcall>
    -->

    <!--
        Profiles only include security settings for the other metadata types
        referenced in the retrieve request. So we build a special
        profile-package.xml that gets the following metadata components together:

        - ApexClass
        - ApexPage
        - CustomApplication
        - CustomObject
        - CustomTab
        - ExternalDataSource
        - Layout
        - Profile

        Then we retrieve profile-package.xml in a separate request in main build.xml
    -->

    <!--
       Bulk Retrieving does not retrieve all the information for the specified
       Metadata Type when compared to using the wildcard in package.xml. So the
       following Metadata Types are listed in package.xml with *

       - CustomObjectTranslation
       - PermissionSet
    -->

    <antcall target="-sfRetrieve"><param name="unpackaged" value="package.xml" /></antcall>
  </target>
</project>
